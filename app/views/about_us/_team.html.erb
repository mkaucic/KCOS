<h1>Tim</h1>
<div class="team__wrapper">
<div class="team__name">
  <% @team.each_with_index do |member, index| %>
    <button><%= member.name %></button>
  <% end %> 
</div>

<div class="team__content">
<% @team.each_with_index do |member, index| %>
    <div class ="team__image">
    <% if member.team_images.blank? %>
      <% unless member.image.blank? %>
        <%= image_tag member.image %>
      <% end %>
    <% else %>
      <% member.team_images.each do |team_image| %>
        <%= image_tag team_image.image %>
      <% end %>
    <% end %>
    </div>
    <div class="team__role">
      <%= member.role %>
    </div>
    <div class="team__selected">
      tu stoji ime osobe
    </div>
    <div class="team__bio">
      <%= sanitize member.bio.html_safe %>
    </div>
    <% end %>
  </div>
</div>